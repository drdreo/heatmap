{
    "name": "@drdreo/heatmap-leaflet",
    "version": "1.0.0",
    "description": "Leaflet integration for @drdreo/heatmap - render heatmaps on interactive maps",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "dev": "tsdown --watch",
        "build": "tsdown",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "peerDependencies": {
        "@drdreo/heatmap": "^1.0.0",
        "leaflet": "^2.0.0"
    },
    "devDependencies": {
        "@drdreo/heatmap": "workspace:*",
        "leaflet": "^2.0.0-alpha.1",
        "tsdown": "latest",
        "typescript": "~5.9.3",
        "vitest": "^4.0.15"
    },
    "author": "DrDreo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drdreo/heatmap.git",
        "directory": "packages/leaflet"
    },
    "bugs": {
        "url": "https://github.com/drdreo/heatmap/issues"
    },
    "homepage": "https://drdreo.github.io/heatmap/",
    "files": [
        "dist",
        "README.md"
    ],
    "keywords": [
        "heatmap",
        "leaflet",
        "map",
        "visualization",
        "canvas",
        "webgl",
        "geospatial"
    ]
}
